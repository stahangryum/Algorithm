   "source": [
    "A, B = map(int, input().split())\n",
    "C = int(input())\n",
    "\n",
    "time = A*60 + B + C\n",
    "\n",
    "time = list(divmod(time, 60))\n",
    "\n",
    "while True:\n",
    "    if time[0] >= 24: time[0] = time[0] - 24\n",
    "    else: break\n",
    "\n",
    "print(' '.join(map(str, time)))"
